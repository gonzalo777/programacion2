#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main()
{
float a,b,c,d,e,f,x,y; 
cout<<"ingrese los datos de la ecuacion 1";
cin>> a >> b >> c ;
cout<<"ingrese los datos de la ecuacion 2";
cin>> d >> f >> e ;
cout<<"entonces las ecuaciones tendrian la siguiente forma :";
cout<<a<<"*x"<<"+"<<b<<"*y"<<"="<<c;
cout<<d<<"*x"<<"+"<<f<<"*y"<<"="<<e;
y=((e-d*c/a)/(f-d*b/a));
x=(c-(b*y))/a;
cout<<"los resultados de la SEL son: "<<"\nx= "<<x<<"\ny= "<<y;
_getch();
}
